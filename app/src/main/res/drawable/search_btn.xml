<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false" >
        <shape>
            <gradient
                android:angle="45"
                android:endColor="@color/yancy_red600"
                android:startColor="@color/yancy_orange600" />
            <corners android:radius="8dp"></corners>
        </shape>
    </item>
    <item android:state_pressed="true"  >
        <shape>
            <gradient
                android:angle="45"
                android:endColor="@color/yancy_red900"
                android:startColor="@color/yancy_orange900" />
            <corners android:radius="8dp"></corners>
        </shape>
    </item>
</selector>